  const setTheme = (theme) =>
    (document.documentElement.dataset.colorScheme = theme);

  const devicePreference = () => {
    setTheme(
      window.matchMedia("(prefers-color-scheme: dark)").matches
        ? "dark"
        : "light",
    );

    window
      .matchMedia("(prefers-color-scheme: dark)")
      .addEventListener("change", (e) => {
        setTheme(e.matches ? "dark" : "light");
      });
  };

  const theme = () => {
    try {
      const localStorageSettings = localStorage.getItem("VG_USER_SETTINGS");
      if (!localStorageSettings) return;

      const settings = JSON.parse(localStorageSettings);

      if (settings?.darkmode?.value === "os") return devicePreference();

      const isDarkmode =
        settings?.darkmode?.value === "true" ||
        settings?.darkmode?.value === true;

      setTheme(isDarkmode ? "dark" : "light");
    } catch (err) {
      console.log("Failed fetching darkmode setting from cache", err);
    }
  };

  const darkmode = () => {
    if (device.endsWith("app")) {
      return devicePreference();
    }

    theme();

    window.addEventListener("darkmode:detect-and-apply", () => {
      theme();
    });

    window.addEventListener("vg:user-settings:set", ({ detail }) => {
      const { data } = detail;
      if ("darkmode" in data) {
        theme();
      }
    });
  };

  darkmode();
})();</script> <script type="module" src="https://www.vg.no/vgc/cdn/vgno/assets/production/_astro/Darkmode.astro_astro_type_script_index_0_lang.eU7rU-8B.js"></script><script>
